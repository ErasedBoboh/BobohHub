repeat wait() until game:IsLoaded()

if game.PlaceId == 12548209 and game:GetService("Workspace"):FindFirstChild("Map") then
    local LocalPlayer = game:GetService("Players").LocalPlayer
    local Autofarm = false
    local Autoquest = false
    local Target = nil

    local Loader = loadstring(game:HttpGet("https://raw.githubusercontent.com/Loloresh/LXWare/main/Library"))()

    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer

    local UI = Loader.new(true)
    UI.ChangeToggleKey(Enum.KeyCode.RightControl)

    local Category = UI:Category("General")

    local Sector = Category:Sector("Autofarm")

    Sector:Cheat(
	"Checkbox",
	"Enabled",
	function(Value)
        
	end)

    Sector:Cheat(
	"Checkbox",
	"Auto Quest",
	function(Value)
        Autoquest = Value
	end)

    game.CoreGui:WaitForChild("FinityUI")

    game.CoreGui.FinityUI.Container.Topbar.TopbarTip.Text = "BoboHub | Boboh#1224"
else
    local LocalPlayer = game:GetService("Players").LocalPlayer
    local Autofarm = false
    local Target = nil

    local Loader = loadstring(game:HttpGet("https://raw.githubusercontent.com/Loloresh/LXWare/main/Library"))()

    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer

    local UI = Loader.new(true)
    UI.ChangeToggleKey(Enum.KeyCode.RightControl)

    local Category = UI:Category("General")

    local Sector = Category:Sector("Autofarm")

    Sector:Cheat(
	"Checkbox",
	"Enabled",
	function(Value)
        Autofarm = Value

        if Value then
            for i,v in pairs(game:GetService("Workspace").Living:GetChildren()) do
                if v.Name ~= LocalPlayer.Name then
                    Target = v
                end
            end
        else
            Target = nil
        end
	end)

    while wait() do
        if Autofarm and Target ~= nil and Target:FindFirstChild("HumanoidRootPart") and Target:FindFirstChild("Humanoid") then
            if Target.Humanoid.Health ~= 0 then
                LocalPlayer.Character.HumanoidRootPart.CFrame = Target.HumanoidRootPart.CFrame * CFrame.new(0, 0, 7.5)

                game:GetService("VirtualInputManager"):SendMouseButtonEvent(0, 0, 0, Autofarm, workspace, 0)
            else
                Target = nil
            end
        end
    end

    game.CoreGui:WaitForChild("FinityUI")

    game.CoreGui.FinityUI.Container.Topbar.TopbarTip.Text = "BoboHub | Boboh#1224"
end
